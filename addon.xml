<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<addon id="script.custom.launcher" name="Amazon Prime Video" version="1.0.0" provider-name="nwgat">
	<requires>
		<import addon="xbmc.python" version="2.14.0"/>
	</requires>
	<extension point="xbmc.python.script" library="addon.py">
		<provides>executable</provides>
	</extension>
	<extension point="xbmc.addon.metadata">
		<platform>all</platform>
		<summary lang="en">Amazon Prime Video</summary>
		<description lang="en">Google Chrome launcher</description>
		<license>GNU General Public License, v2</license>
		<language></language>
		<forum>http://nwgat.ninja</forum>
		<source>https://github.com/nwgat/kodi-custom-launcher</source>
		<website>nwgat.ninja</website>
		<email>hello@nwgat.ninja</email>
		<assets>
			<icon>icon.png</icon>
			<fanart>fanart.jpg</fanart>
			<screenshot>resources/screenshot-01.jpg</screenshot>
			<screenshot>resources/screenshot-02.jpg</screenshot>
			<screenshot>resources/screenshot-03.jpg</screenshot>
		</assets>
		<news>first release</news>
	</extension>
</addon>
